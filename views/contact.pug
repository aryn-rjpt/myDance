extends base.pug

block style 
    style
        include ../static/style.css
        include ../static/styleC.css

block content 
    h1 Contact Us 
    form(action="/contact" method='post' id='contact')
        input(type='text' class='myInput' name='name' placeholder='Enter your name')
        input(type='text' class='myInput' name='age' placeholder='Enter your age')
        input(type='text' class='myInput' name='email' placeholder='Enter your email')
        input(type='text' class='myInput' name='mobile' placeholder='Enter your mobile')
        textarea(rows="10" class='myInput' name='message' placeholder='Enter your message')
        button.btn.myInput(type="submit") Submit
